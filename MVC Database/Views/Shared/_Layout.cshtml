<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/vendors/css/bootstrap.css" rel="stylesheet" />
    <link href="~/css/site.css" rel="stylesheet" />
    <script src="~/vendors/js/bootstrap.js"></script>
    <script src="~/vendors/js/jquery-3.3.1.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/js/site.js"></script>
</head>
<body>

    <header id="header" class="clearfix">
        <img src="~/image/logo.gif" height="30" alt="Logo" />
        @{
            await Html.RenderPartialAsync("_Navbar");
        }
    </header>


    <div id="body">
        <div id="bodyArea">
            @RenderBody()
        </div>
    </div>

    <footer>
        <div id="footer">Copyright @DateTime.Now.ToString("yyyy") &copy; Mats</div>
    </footer>


</body>
</html>
